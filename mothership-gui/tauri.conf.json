{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Mothership GUI",
  "version": "0.1.0",
  "identifier": "com.mothership.gui",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "app": {
    "security": {
      "csp": null,
      "capabilities": ["main-capability"]
    },
    "windows": [
      {
        "label": "main",
        "fullscreen": false,
        "height": 800,
        "resizable": true,
        "title": "Mothership GUI",
        "width": 1200,
        "center": true,
        "minHeight": 600,
        "minWidth": 800
      }
    ]
  },
  "bundle": {
    "active": true,
    "category": "DeveloperTool",
    "copyright": "Copyright Â© 2024 Mothership Team",
    "icon": [
      "icons/icon.png"
    ],
    "longDescription": "Mothership GUI - A revolutionary version control system with real-time collaboration",
    "shortDescription": "Mothership GUI",
    "targets": "all",
    "externalBin": [],
    "resources": [],
    "linux": {
      "deb": {
        "depends": []
      }
    },
    "macOS": {
      "entitlements": null,
      "exceptionDomain": "",
      "frameworks": [],
      "providerShortName": null,
      "signingIdentity": null
    },
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    }
  },
  "plugins": {}
} 